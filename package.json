{
    "name": "qua",
    "version": "0.0.1",
    "author": "CalvinVon",
    "private": true,
    "main": "main.js",
    "homepage": ".",
    "scripts": {
        "start": "react-scripts start",
        "start:electron": "electron .",
        "build": "react-scripts build",
        "build:electron": "electron-builder",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "build": {
        "appId": "com.electron.qua",
        "directories": {
            "buildResources": "resources",
            "output": "release"
        },
        "files": [
            "build/*",
            "./qua.png"
        ],
        "productName": "Qua",
        "copyright": "Copyright Â© 2019 ${author}",
        "mac": {
            "target": [
                "zip"
            ],
            "icon": "qua.png"
        },
        "win": {
            "target": [
                "nsis",
                "zip"
            ],
            "icon": "qua.png"
        }
    },
    "publish": {
        "provider": "github",
        "owner": "CalvinVon",
        "repo": "qua",
        "private": true
    },
    "dependencies": {
        "@toast-ui/react-image-editor": "^1.0.1",
        "antd": "^3.20.7",
        "canvas-compress": "^1.1.5",
        "classnames": "^2.2.6",
        "electron-is-dev": "^1.1.0",
        "fabric": "^3.3.2",
        "jquery": "^3.4.1",
        "minireset.css": "0.0.5",
        "moment": "^2.24.0",
        "normalize.css": "^8.0.1",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-keep-alive": "^2.1.0",
        "react-router-dom": "^5.0.1",
        "react-scripts": "3.0.1",
        "react-sortablejs": "^1.5.1",
        "sortablejs": "^1.10.0-rc3",
        "tui-image-editor": "^3.7.0"
    },
    "devDependencies": {
        "electron": "^5.0.8",
        "babel-plugin-import": "^1.12.0",
        "electron-builder": "^21.2.0",
        "node-sass": "^4.12.0",
        "sass-loader": "^7.1.0"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}